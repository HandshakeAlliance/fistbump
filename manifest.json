{
  "name": "fistbump",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Privacy preserving search for Handshake TLDs",
  "icons": {
    "16": "static/icon-16.png",
    "128": "static/icon-128.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "notifications",
    "http://*/*",
    "https://*/*"
  ],
  "omnibox": {
    "keyword": "fb"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "static/icon-19.png",
      "38": "static/icon-38.png"
    },
    "default_title": "fistbump",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "static/icon-48.png"
  ]
}
